{
  "name": "hylyf-web-app",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng build --configuration production",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng build --configuration development",
    "postinstall": "ngcc --properties es2015 browser module main",
    "test": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng test",
    "watch": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng build --watch --configuration development",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/cdk": "14.2.0",
    "@angular/common": "14.2.0",
