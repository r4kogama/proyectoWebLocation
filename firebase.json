{
  "hosting": {
    "redirects": [
      {
        "source": "/**",
        "destination": "https://findlyapp.vercel.app",
        "type": 301
      }
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "europe-southwest1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
